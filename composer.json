        "fof/polls": "*",
        "fof/pretty-mail": "*",
        "fof/reactions": "*",
        "fof/share-social": "*",
        "fof/sitemap": "*",
        "fof/socialprofile": "*",
        "fof/spamblock": "*",
        "fof/split": "*",
